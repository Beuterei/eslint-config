{
  "name": "@beuluis/eslint-config",
  "version": "2.0.0",
  "description": "Shared eslint config for my projects",
  "main": "./eslintrc.js",
  "files": [
    "**/*.js",
    "configurations"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "npm": ">8.3"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beuluis/eslint-config.git"
  },
  "author": "Luis Beu <me@luisbeu.de> (https://luisbeu.de/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beuluis/eslint-config/issues"
  },
  "homepage": "https://github.com/beuluis/eslint-config#readme",
  "dependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "eslint-config-canonical": "^41.1.7",
    "eslint-plugin-anti-trojan-source": "^1.1.1",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-i18next": "6.0.3",
    "eslint-plugin-jest-dom": "^5.0.2",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-security-node": "^1.1.1",
    "eslint-plugin-testing-library": "^6.0.1"
  },
  "peerDependencies": {
    "eslint": "^8"
  },
  "devDependencies": {
    "@beuluis/hook-cli": "1.2.1",
    "eslint": "8.47.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1"
  },
  "overrides": {
    "eslint-config-canonical": {
      "eslint-plugin-import": "npm:eslint-plugin-i@2.27.5"
    }
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-shareable-config",
    "config",
    "linting"
  ],
  "lint-staged": {
    "*.{js,json}": [
      "eslint"
    ]
  }
}
