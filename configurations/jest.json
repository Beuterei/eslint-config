{
  "$schema": "https://json.schemastore.org/eslintrc",
  "extends": ["canonical/jest", "plugin:jest-dom/recommended", "@beuluis/eslint-config/prettier"],
  "rules": {
    "func-names": "off"
  },
  "overrides": [
    {
      "files": "*.ts",
      "rules": {
        "@typescript-eslint/unbound-method": "off",
        "jest/unbound-method": "error"
      }
    }
  ]

}
