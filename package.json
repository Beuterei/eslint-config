{
  "name": "@beuluis/eslint-config",
  "version": "1.2.3",
  "description": "Shared eslint config for my projects",
  "main": "./eslintrc.js",
  "files": [
    "**/*.js",
    "configurations"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "npm run lint && npm run format",
    "lint": "eslint .",
    "format": "prettier . --check",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beuluis/eslint-config.git"
  },
  "author": "Luis Beu <me@luisbeu.de> (https://luisbeu.de/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beuluis/eslint-config/issues"
  },
  "homepage": "https://github.com/beuluis/eslint-config#readme",
  "dependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "eslint-config-canonical": "^37.0.3",
    "eslint-plugin-anti-trojan-source": "^1.1.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-regex": "^1.10.0",
    "eslint-plugin-regexp": "^1.9.0",
    "eslint-plugin-security-node": "^1.1.1",
    "eslint-plugin-testing-library": "^5.7.2"
  },
  "peerDependencies": {
    "eslint": "^8"
  },
  "devDependencies": {
    "@beuluis/hook-cli": "1.1.3",
    "@beuluis/prettier-config": "2.0.1",
    "eslint": "8.25.0",
    "husky": "8.0.3",
    "lint-staged": "13.0.3",
    "np": "7.6.2",
    "prettier": "2.7.1"
  },
  "prettier": "@beuluis/prettier-config",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-shareable-config",
    "config",
    "linting"
  ],
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "*.{js,json}": [
      "eslint"
    ]
  },
  "np": {
    "tests": false,
    "publish": false
  }
}
